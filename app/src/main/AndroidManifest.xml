<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.inhatc.garim">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_garim"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_garim_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Garim.NoActionBar"
        tools:replace="android:icon">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/SplashTheme"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".ElectronicSignatureActivity"></activity>
        <activity android:name=".ApplicationStatusActivity"></activity>
        <activity android:name=".SignActivity"></activity>
        <activity android:name=".SigninfoActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".WriteFidoActivity"></activity>
        <activity android:name=".WriteResidenceActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".WriteFinishActivity"></activity>
    </application>

</manifest>